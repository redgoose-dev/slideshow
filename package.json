{
  "name": "@redgoose/vue-slideshow",
  "version": "2.1.10",
  "type": "module",
  "main": "./dist/vue-slideshow.js",
  "module": "./dist/vue-slideshow.js",
  "exports": {
    ".": "./dist/vue-slideshow.js",
    "./style": "./dist/vue-slideshow.css"
  },
  "scripts": {
    "dev": "NODE_ENV=development vite --config config/vite.dev.config.js",
    "docs": "NODE_ENV=development vite --config config/vite.docs.config.js",
    "build-docs": "NODE_ENV=production vite build --config config/vite.docs.config.js --emptyOutDir",
    "build-libs": "bunx --bun vite build --config config/vite.build.config.js --emptyOutDir",
    "build": "bun run build-libs && bun run build-docs"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "@redgoose/json-editor": "^1.2.2",
    "@types/bun": "^1.2.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "bun": "^1.2.3",
    "esbuild": "^0.25.0",
    "esbuild-plugin-vue3": "^0.4.2",
    "lucide": "^0.474.0",
    "pinia": "^3.0.1",
    "sass": "^1.85.0",
    "terser": "^5.39.0",
    "vite": "^6.1.1",
    "vue": "^3.5.13"
  }
}