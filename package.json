{
  "name": "@redgoose/slideshow",
  "productName": "Slideshow",
  "description": "vue's slideshow",
  "version": "1.1.17-ready",
  "main": "src/resource.js",
  "repository": "https://github.com/redgoose-dev/slideshow.git",
  "author": {
    "name": "redgoose",
    "email": "scripter@me.com",
    "url": "https://redgoose.me"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve --host 0.0.0.0 --port 3000",
    "build-docs": "rm -rf docs && vue-cli-service build --target app --dest docs src/main.js",
    "build-lib": "rm -rf dist && vue-cli-service build --target lib --name slideshow --dest dist --formats commonjs src/resource.js",
    "dev-external": "vue-cli-service serve --host 0.0.0.0 --port 3000 src/external/main.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "vue": "^3.2.11",
    "vue-i18n": "^9.1.7",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.11",
    "sass": "^1.39.0",
    "sass-loader": "^10"
  }
}
